{
  "name": "ztdocs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "preview": "env-cmd tinacms dev -c 'docusaurus start --host 0.0.0.0'",
    "start": "env-cmd docusaurus start --host 0.0.0.0",
    "build": "env-cmd docusaurus build",
    "copy-tina": "cp -r tina/__generated__ build/",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "screenshot": "playwright test",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "generate:api": "env-cmd docusaurus gen-api-docs all",
    "lint": "run-s -c lint:config lint:markdown lint:spelling",
    "lint:config": "jsonlint -q cspell.json",
    "lint:markdown": "markdownlint-cli2 \"docs/**/*.md\"",
    "lint:spelling": "cspell --validate-directives --no-progress docs/**/*.md"
  },
  "resolutions": {
    "postman-code-generators": "1.10.0"
  },
  "dependencies": {
    "@cspell/dict-markdown": "^2.0.2",
    "@cspell/dict-software-terms": "^4.1.5",
    "@docusaurus/core": "^3.8.1",
    "@docusaurus/faster": "^3.8.1",
    "@docusaurus/plugin-client-redirects": "^3.8.1",
    "@docusaurus/plugin-content-docs": "^3.8.1",
    "@docusaurus/preset-classic": "^3.8.1",
    "@docusaurus/theme-common": "^3.8.1",
    "@docusaurus/theme-mermaid": "^3.8.1",
    "@docusaurus/utils": "^3.8.1",
    "@mdx-js/react": "^3.1.0",
    "@playwright/test": "^1.49.0",
    "@prantlf/jsonlint": "^16.0.0",
    "@tinacms/cli": "^1.10.1",
    "@types/react": "^18.3.11",
    "async-folder-walker": "^3.0.5",
    "clsx": "^2.1.1",
    "cspell": "^8.14.4",
    "docusaurus-plugin-openapi-docs": "^4.5.1",
    "docusaurus-theme-openapi-docs": "^4.5.1",
    "markdownlint-cli2": "^0.15.0",
    "npm-run-all2": "^6.2.3",
    "pixelmatch": "^6.0.0",
    "pngjs": "^7.0.0",
    "posthog-docusaurus": "^2.0.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tinacms": "^2.8.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
