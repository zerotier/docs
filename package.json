{
  "name": "ztdocs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "NODE_OPTIONS=--openssl-legacy-provider docusaurus start --host 0.0.0.0",
    "build": "NODE_OPTIONS=--openssl-legacy-provider docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "lint": "run-s lint:*",
    "lint:config": "jsonlint cspell.json > /dev/null",
    "lint:markdown": "markdownlint-cli2 docs/**/*.md --fix",
    "lint:spelling": "cspell docs/**/*.md"
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-beta.0",
    "@docusaurus/preset-classic": "2.0.0-beta.0",
    "@mdx-js/react": "^1.6.22",
    "clsx": "^1.1.1",
    "cspell": "^6.19.2",
    "docusaurus-plugin-openapi": "0.0.16",
    "docusaurus-theme-redoc": "^0.4.2",
    "jsonlint": "^1.6.3",
    "markdown-spellcheck": "^1.3.1",
    "markdownlint": "^0.27.0",
    "markdownlint-cli": "^0.33.0",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "redocusaurus": "^0.4.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "markdownlint-cli2": "^0.8.1"
  }
}
