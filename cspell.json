{
  "caseSensitive": false,
  "ignorePaths": [
    "docs/zerotier/ztintro.md"
  ],
  "dictionaryDefinitions": [
    {
      "name": "custom-words",
      "path": "./custom-words.txt",
      "addWords": true
    }
  ],
  "dictionaries": [
    "companies",
    "custom-words",
    "networkingTerms",
    "publicLicensese",
    "softwareTerms"
  ],
  "import": ["@cspell/dict-markdown/cspell-ext.json", "@cspell/dict-software-terms/cspell-ext.json"],
  "languageSettings": [
    {
      "languageId": "markdown",
      "ignoreRegExpList": [
        "'s\\b",
        "zt\\w+",
        "\\$\\w+",
        "/^\\s*```[\\s\\S]*?^\\s*```/gm",
        "/`[\\s\\S]*?`/",
        "/\\[(\\S| )+\\]/",
        "/\\{\\#.*\\}/gm",
        "/\\{\\*\\}/gm"
      ]
    }
  ]
}
